{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "public/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_KEY": "@app_key",
    "SANCTUM_STATEFUL_DOMAINS": "*.vercel.app,localhost,127.0.0.1"
  },
  "functions": {
    "public/index.php": {
      "runtime": "@vercel/php@0.6.0"
    }
  }
}